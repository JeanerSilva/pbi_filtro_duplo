{
  "privileges": [],
  "dataRoles": [
    { "name": "level1", "kind": "Grouping", "displayName": "Nível 1" },
    { "name": "level2", "kind": "Grouping", "displayName": "Nível 2" },
    { "name": "level3", "kind": "Grouping", "displayName": "Nível 3" }
  ],
  "dataViewMappings": [
    {
      "categorical": {
        "categories": {
          "select": [
            { "for": { "in": "level1" } },
            { "for": { "in": "level2" } },
            { "for": { "in": "level3" } }
          ],
          "dataReductionAlgorithm": { "window": { "count": 50000 } }
        }
      }
    }
  ],
  "objects": {
    "general": {
      "displayName": "Geral",
      "properties": {
        "filter": { "type": { "filter": true } },
        "selfFilter": { "type": { "filter": { "selfFilter": true } } }
      }
    },
    "behavior": {
      "displayName": "Comportamento",
      "properties": {
        "selectionMode": { "displayName": "Seleção única", "type": { "bool": true } },
        "forceSelection": { "displayName": "Forçar seleção (modo único)", "type": { "bool": true } },
        "leafOnly": { "displayName": "Selecionar apenas folhas", "type": { "bool": true } }
      }
    },
    "formatting": {
      "displayName": "Formatação",
      "properties": {
        "fontSize":     { "displayName": "Tamanho da fonte (lista)", "type": { "formatting": { "fontSize": true } } },
        "itemPadding":  { "displayName": "Espaçamento do item (px)", "type": { "numeric": true } },

        "fontFamily":   { "displayName": "Fonte", "type": { "formatting": { "fontFamily": true } } },
        "fontColor":    { "displayName": "Cor",   "type": { "fill": { "solid": { "color": true } } } },
        "fontBold":         { "displayName": "Negrito",     "type": { "bool": true } },
        "fontItalic":       { "displayName": "Itálico",     "type": { "bool": true } },
        "fontUnderline":    { "displayName": "Sublinhado",  "type": { "bool": true } }
      }
    },
    "search": {
      "displayName": "Busca",
      "properties": {
        "enabled":     { "displayName": "Exibir busca", "type": { "bool": true } },
        "placeholder": { "displayName": "Placeholder",   "type": { "text": true } },
        "fontSize":    { "displayName": "Tamanho da fonte (busca)", "type": { "formatting": { "fontSize": true } } }
      }
    }
  },
  "supportsHighlight": false,
  "suppressDefaultTitle": true,
  "supportsSynchronizingFilterState": true
}
